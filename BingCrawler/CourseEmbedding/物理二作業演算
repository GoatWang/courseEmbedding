58��
          
��-��-�ˤ���

          ��2013/06/23(��) 15:46:09 ID:???
        

          
�ҤȤޤ�������ʸ�򤵤�ä��ɤ���Ȥ��ѿ��μ¸������ǥ��ݤ��Τ��ɤ߼�ä��Τ�
�����餯���ʤ�������С����������ѿ��μ¸������ɤ�񤤤Ƥߤ�


;�ѿ��Υꥹ��
;  ((ING . c) . (((1 2 3) . b)  . (((x y z) . a) . (����NIL�ν�ꥹ�Ȥʤ�NIL�Ǥ�ʤ�Ǥ�))))

;findvar �ѿ��ꥹ��args���ѿ�̾symbol�������T���ʤ����NIL���֤�
(label findvar (lambda (args symbol)
 (cond 
  ((not (atom args)) (cond
   ((not (atom (car args))) (cond
    ((atom (cdar args)) (cond
     ((eq (cdar args) symbol)  (quote T))
     ((quote t) (findvar (cdr args) symbol))))
    ((quote t) (quote NIL))))
   ((quote t) (quote NIL))))
  ((quote t) (quote NIL)))))

;repvar �ѿ��ꥹ��args���ѿ�̾symbol������а��ּ����ˤ��뤽���ѿ����ͤ�value���֤��������ѿ��ꥹ�Ȥ��֤����ʤ���и����ѿ��ꥹ�Ȥ򤽤Τޤ��֤�
(label repvar (lambda (args symbol value)
 (cond 
  ((not (atom args)) (cond
   ((not (atom (car args))) (cond
    ((atom (cdar args)) (cond
     ((eq (cdar args) symbol)  (cons (cons value symbol) (cdr args)))
     ((quote t) (cons (car args) (repvar (cdr args) symbol value)))))
    ((quote t) args)))
   ((quote t) args)))
  ((quote t) args))))

;delvar �ѿ��ꥹ��args���ѿ�̾symbol������а��ּ����ˤ��뤽��̾���ѿ����������ѿ��ꥹ�Ȥ��֤����ʤ���и����ѿ��ꥹ�Ȥ򤽤Τޤ��֤�
(label delvar (lambda (args symbol)
 (cond 
  ((not (atom args)) (cond
   ((not (atom (car args))) (cond
    ((atom (cdar args)) (cond
     ((eq (cdar args) symbol)  (cdr args))
     ((quote t) (cons (car args) (delvar (cdr args) symbol)))))
    ((quote t) args)))
   ((quote t) args)))
  ((quote t) args))))

;addvar �ѿ��ꥹ��args�ΰ��ּ������ѿ�̾symbol��value���ѿ����ɲä���
(label addvar (lambda (args symbol value)
 (cons (cons value symbol) args)))

;setvar �ѿ��ꥹ��args���ѿ�̾symbol��������ͤ�value���֤������롢�ʤ���п������ɲä���
(label setvar (lambda (args symbol value)
 (cond 
  ((findvar args symbol) (repvar args symbol value))
  ((quote t) (addvar args symbol value)))))

;getvar �ѿ��ꥹ��args���ѿ�̾symbol������а��ּ����ˤ��뤽���ѿ����ͤ��֤����ʤ���Х���ܥ�ERROR���֤�
(label getvar (lambda (args symbol)
 (cond 
  ((not (atom args)) (cond
   ((not (atom (car args))) (cond
    ((atom (cdar args)) (cond
     ((eq (cdar args) symbol)  (caar args))
     ((quote t) (getvar (cdr args) symbol))))
    ((quote t) (quote ERROR))))
   ((quote t) (quote ERROR))))
  ((quote t) (quote ERROR))))) 



輸入文字註意(flash)
1. 勿選取 "自動微調"                                                                     
        2.向上下翻動  f_disp.scroll = f_disp.scroll-1;


aspx<-->as30
var lx = 172, ly=309 , rx=372, ry =309, bx=50,by=60;
 
        addEventListener(Event.ENTER_FRAME,aa) 
        function aa(e:Event):void{ 
	if(mouseX>0 && mouseX<300){ 
	itnl.x = mouseX 
    lx=mouseX 
	varlx.text =lx 
	ly=mouseX 
	varly.text =ly 
 }

 
        }
 
        var a_URL:URLRequest= new URLRequest("l.aspx"); 
        var send_data:URLVariables = new URLVariables() 
        a_URL.method = URLRequestMethod.POST; 
        a_URL.data = send_data;
 
        var a_loader:URLLoader = new URLLoader; 
        a_loader.dataFormat = URLLoaderDataFormat.VARIABLES;
 
        a_loader.addEventListener(Event.COMPLETE,bb);
 
        function bb(e:Event):void{ 
        rx = a_loader.data.rx 
        varrx.text=rx 
        itnr.x=rx 
        ry = a_loader.data.ry 
        varry.text=ry 
        itnr.y=ry 
        bx = a_loader.data.bx 
        varbx.text=bx 
        itnb.x=bx 
        by = a_loader.data.by 
        varby.text=by 
        itnb.y=by
 
        send_data.lx = lx 
        send_data.ly = ly 
        }
 
        setInterval(cc,1000) 
        function cc(){ 
        a_loader.load(a_URL) 
        }





asp.net mail
 Dim Mail As New Net.Mail.MailMessage            
        Mail.From = New Net.Mail.MailAddress("mm@mail.ntua.edu.tw", "mm")            
        Mail.Subject = "1234567"           
        Mail.IsBodyHtml = True            
        Mail.BodyEncoding = System.Text.Encoding.UTF8            
        Mail.SubjectEncoding = System.Text.Encoding.UTF8            
        Mail.Body = "7654321"           
        Mail.To.Add("dd@mail.ntua.edu.tw")          
        Dim SMTPServer As New Net.Mail.SmtpClient            
        SMTPServer.Host = "mail.ntua.edu.tw"           
        SMTPServer.Port = "25"            
        SMTPServer.DeliveryMethod = Net.Mail.SmtpDeliveryMethod.Network            
        SMTPServer.Send(Mail)          
        Label1.Text = "mail ok!"


db
<%@ Import NameSpace="System.Data.OleDb"          
        %>         
        -----
(select)         
        Dim Conn As New OleDbConnection()                 
        Dim ConStr, QueryStr As String                 
        Dim i, j As Short                 
        ConStr = "Provider=Microsoft.Jet.OLEDB.4.0;"                 
        ConStr += "Data Source=" & Server.MapPath("gsb.mdb")                 
        QueryStr = "Select * From gsbdata where a_title='b' "                 
        Conn.ConnectionString = ConStr                 
        Conn.Open()
         
        Dim MyCommand As New OleDbCommand()                 
        MyCommand.CommandText = QueryStr                 
        MyCommand.Connection = Conn                 
        Dim MyReader As OleDbDataReader                 
        MyReader = MyCommand.ExecuteReader()                 
        MyReader.Read()
         
        Label1.Text = " "
         
        Do
         
        Label1.Text = Label1.Text & MyReader.GetValue(1) &                  
        "--" & MyReader.GetValue(2) & "--" &                  
        MyReader.GetValue(3) & "<hr>"

         
        Loop While MyReader.Read
         
        MyCommand.Dispose()         
        Conn.Close()         
        -------         
        (insert)(delete)(update)         
        Dim ConStr, CmdStr, i                  
        Dim Conn As New System.Data.OleDb.OleDbConnection()                  
        ConStr="Provider=Microsoft.Jet.OLEDB.4.0;"         
        ConStr+="Data Source=" & Server.MapPath("db2.mdb")                  
        CmdStr = "INSERT INTO 通訊錄(姓名,電話) values ('王王','456')"                  
        Conn.ConnectionString=ConStr         
        Conn.Open()         
        Dim Cmd As New System.Data.OleDb.OleDbCommand()                  
        Cmd.CommandText=CmdStr         
        Cmd.connection=conn         
        i=cmd.ExecuteNonQuery()         
        Cmd.Dispose()         
        Conn.Close()         
        ------         
        (insert)INSERT INTO 通訊錄(姓名,電話) values ('王王','456')                  
        (delete)Delete From 通訊錄Where 電話='456'                  
        (update)update 通訊錄set 電話='123' Where 分類='同學'


as3.0->aspx
var send_data:URLVariables = new URLVariables();    
        send_data.dataA = "ABC";    
        send_data.dataB = 123;    
        var a_URL:URLRequest = new URLRequest("sample.aspx");    
        a_URL.method = URLRequestMethod.POST;    
        a_URL.data = send_data;    
        sendToURL(a_URL);
-----   
        <%
Application("my1") =    
        Request("dataA")   
        Application("my2") = Request("dataB")   
        %>


aspx->as3.0
aaa=變數A&bbb=變數B     
        (<%    response.write("....") %>)   
        ----   
        var a_loader:URLLoader = new URLLoader();    
        a_loader.dataFormat = URLLoaderDataFormat.VARIABLES;    
        a_loader.addEventListener(Event.COMPLETE, doComplete);    
        function doComplete(e:Event):void {    
	trace(a_loader.data.aaa);    
	trace(a_loader.data.bbb);    
        }    
        var a_URL:URLRequest = new URLRequest("data2.txt");    
        a_loader.load(a_URL);






　
　
　



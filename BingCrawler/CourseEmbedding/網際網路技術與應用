          flash-->aspx         
 Application("myx") =                                 
          Request("varx")          
 -----          
        aspx-->flash           
          Response.Write("varxx=" &                                 
          Application("myx"))          
          Response.Write("varxx=" &                                 
          Application("myx") & "&" & "varyy="                                
          & Application("myy"))                            
 -----             

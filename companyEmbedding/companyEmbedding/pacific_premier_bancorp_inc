function selectRecordType()
{
	var source = document.getElementById("recordType");
	var recordType = source.options[source.selectedIndex].text;
	
	var select = document.getElementById('00N3A00000CKktH');
	
	select.options.length = 0;
    
    if (recordType == "--None--")
	{
		var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "N/A";
    	opt.selected = true;
    	select.appendChild(opt);
    }
    else
    {
    	var opt = document.createElement('option');
    	opt.value = "";
    	opt.innerHTML = "--None--";
    	select.appendChild(opt);
    }
    
	if (recordType == "Banking Services")
	{
		var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Commercial";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Consumer";
    	select.appendChild(opt);
    }
    
    if (recordType == "HOA")
	{
		var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Commercial";
    	opt.selected = true;
    	select.appendChild(opt);
    }
    
    if (recordType == "Loan Services")
	{
		var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Commercial";
    	opt.selected = true;
    	select.appendChild(opt);
    }
    
    if (recordType == "Treasury Management Services")
	{
		var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Commercial";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Consumer";
    	select.appendChild(opt);
    }
	
	var select = document.getElementById('00N3A00000CKktG');
	
	select.options.length = 0;
	
	var opt = document.createElement('option');
    opt.value = "";
    opt.innerHTML = "N/A";
    select.appendChild(opt);
}

function selectCustomerType()
{
	var source = document.getElementById("recordType");
	var recordType = source.options[source.selectedIndex].text;
	
	var source = document.getElementById("00N3A00000CKktH");
	var customerType = source.options[source.selectedIndex].text;
	
	var select = document.getElementById('00N3A00000CKktG');
	
	select.options.length = 0;
    
    if (recordType != "Treasury Management Services" || customerType == "--None--")
    {
    	var opt = document.createElement('option');
    	opt.value = "";
    	opt.innerHTML = "N/A";
    	select.appendChild(opt);
    }
    
	if (recordType == "Treasury Management Services" && customerType == "Commercial")
	{
		var opt = document.createElement('option');
    	opt.value = "";
    	opt.innerHTML = "--None--";
    	select.appendChild(opt);
    	
		var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "ACH Origination";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "ACH Positive Pay";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Bill Pay";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Business eBanking";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Lockbox";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Mobile Banking";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Mobile Remote Deposit";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Other";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Positive Pay";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Remote Deposit";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Wire Transfers";
    	select.appendChild(opt);
    }
    
    if (recordType == "Treasury Management Services" && customerType == "Consumer")
	{
		var opt = document.createElement('option');
    	opt.value = "";
    	opt.innerHTML = "--None--";
    	select.appendChild(opt);
    	
		var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "ATM/Debit Card";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Bill Pay";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Mobile Banking";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Mobile Remote Deposit";
    	select.appendChild(opt);
    	
    	var opt = document.createElement('option');
    	opt.value = opt.innerHTML = "Online Banking";
    	select.appendChild(opt);
    }
}










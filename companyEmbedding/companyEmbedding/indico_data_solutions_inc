# single example
emotion(
    "I did it. I got into Grad School. Not just any program, but a GREAT program. :-)",
    api_key = 'YOUR_API_KEY'
)

# batch example
emotion(
    c(
        "I did it. I got into Grad School. Not just any program, but a GREAT program. :-)",
        "Like seriously my life is bleak, I have been unemployed for almost a year."
    ),
    api_key = 'YOUR_API_KEY'
)




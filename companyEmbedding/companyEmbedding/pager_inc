  "url": "https://api.pagerduty.com/incidents",
  "method": "GET",
  "headers": {
    "Accept": "application/vnd.pagerduty+json;version=2",
    "Authorization": "Token token=E7px6VVr3PVHZPJq51oa"
  }
};

$.ajax(settings).done(function (response) {
  console.log(response);
});



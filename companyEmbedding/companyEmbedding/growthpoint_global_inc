		var w = $(window).width();
		alert(o.id);
		if(w > 640) {
			$.colorbox({
				href:$("#" + o.id).attr("href"),
				inline:true, 
				innerWidth:640, 
				innerHeight:390
			});
		}
		else {
			$.colorbox({
				href:url,
				inline:true, 
				innerWidth:"80%", 
				innerHeight:200
			});
		}
		$('#gallery').data('nivoGallery').pause();
	}





